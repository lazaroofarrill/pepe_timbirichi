<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- #3(HBL de Carga Transcargo)-->
        <template id="store_report_product">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.internal_layout">
                        <br/><br/><br/>
                        <h1>Reporting</h1><br/>
                        Name: <t t-esc="o.name"/><br/>
                        Currency: <t t-esc="o.currency_id.name"/><br/>
                        State: <t t-esc="o.state"/><br/>
                        Price: <t t-esc="o.retail_price"/><br/>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>